##Return data
id <- "002"
directory <- "C:/Users/Adam/Desktop/Computing for Data Analysis/specdata"
summarize=TRUE

getmonitor <- function(id, directory, summarize=FALSE){
	filename <- paste0(directory,"/",id,".csv")	
	data <- read.csv(filename, header=TRUE)
	if(summarize==TRUE){print(summary(data))}
	return(data)		
}

data <- getmonitor("101", "C:/Users/Adam/Desktop/Computing for Data Analysis/specdata", summarize=TRUE)
head(data)


